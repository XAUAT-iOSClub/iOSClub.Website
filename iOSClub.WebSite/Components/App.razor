@inject IWebHostEnvironment WebEnvironment

<!DOCTYPE html>
<html lang="en">

<head>
    <title>西安建筑科技大学iOS众创空间俱乐部</title>
    <meta charset="utf-8"/>
    <meta name="viewport" content="width=device-width, initial-scale=1.0"/>
    <meta name="keywords" content="西建大ios club,西建大iOSClub,西建大iOS Club,西安建筑科技大学iOS众创空间俱乐部,西安建筑科技大学,iOS Club ">
    <meta name="description" content="西安建筑科技大学iOS众创空间俱乐部，是由Apple公司资金支持，受南山书院和西安建筑科技大学大学生创新创业实践中心指导的学术型学生团体">
    <base href="/"/>
    <link rel="canonical" href="https://www.xauat.site"/>
    <meta http-equiv="content-language" content="zh-cn">
    <link rel="stylesheet" href="app.css?v=@DateTime.Today.Ticks"/>
    <link rel="icon" type="image/png" href="iOS_Club_LOGO.png"/>
    <link href="_content/AntDesign/css/ant-design-blazor.css" rel="stylesheet">
    <HeadOutlet @rendermode="InteractiveServer"/>
</head>
<!-- Google tag (gtag.js) -->
@if (WebEnvironment.IsProduction())
{
    <script async src="https://www.googletagmanager.com/gtag/js?id=@(Environment.GetEnvironmentVariable("GOOGLE", EnvironmentVariableTarget.Process))"></script>
    <script>
    window.dataLayer = window.dataLayer || [];
    function gtag(){dataLayer.push(arguments);}
    gtag('js', new Date());

    gtag('config', '@(Environment.GetEnvironmentVariable("GOOGLE", EnvironmentVariableTarget.Process))');
</script>
}
<body>
<Routes @rendermode="InteractiveServer"/>
<AntContainer @rendermode="InteractiveServer"/>
<script src="https://unpkg.com/@@antv/g2plot@1.1.28/dist/g2plot.js"></script>
<script src="_content/AntDesign.Charts/ant-design-charts-blazor.js"></script>
<script src="_framework/blazor.web.js"></script>
<script src="_content/AntDesign/js/ant-design-blazor.js"></script>
<script src="JS/site.js?v=@DateTime.Today.Ticks"></script>
<script src="JS/markdown-it.min.js"></script>
<script src="JS/markdown-it-container.min.js"></script>
<script src="JS/markdown-it-anchor.js"></script>
<script src="JS/mermaid.min.js"></script>

</body>

</html>